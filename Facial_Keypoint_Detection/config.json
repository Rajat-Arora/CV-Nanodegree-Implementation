{
  "model": {
    "name": "FacialKeypointsNet",
    "input_size": 224,
    "input_channels": 1,
    "output_size": 136,
    "num_keypoints": 68,
    "architecture": {
      "conv_layers": [
        {"in_channels": 1, "out_channels": 32, "kernel_size": 5},
        {"in_channels": 32, "out_channels": 64, "kernel_size": 5},
        {"in_channels": 64, "out_channels": 128, "kernel_size": 5},
        {"in_channels": 128, "out_channels": 256, "kernel_size": 5}
      ],
      "fc_layers": [25600, 1024, 512, 136],
      "dropout_rate": 0.3
    }
  },
  "training": {
    "batch_size": 16,
    "num_epochs": 50,
    "learning_rate": 0.001,
    "optimizer": "Adam",
    "loss_function": "MSELoss",
    "scheduler": {
      "type": "ReduceLROnPlateau",
      "factor": 0.5,
      "patience": 5
    },
    "early_stopping_patience": 10
  },
  "data": {
    "train_csv": "path/to/training_frames_keypoints.csv",
    "train_dir": "path/to/training/",
    "val_csv": "path/to/validation_frames_keypoints.csv",
    "val_dir": "path/to/validation/",
    "test_csv": "path/to/test_frames_keypoints.csv",
    "test_dir": "path/to/test/"
  },
  "transforms": {
    "rescale_size": 250,
    "crop_size": 224,
    "normalize": {
      "mean_keypoints": 100,
      "std_keypoints": 50
    }
  },
  "inference": {
    "face_cascade": "detector_architectures/haarcascade_frontalface_default.xml",
    "scale_factor": 1.2,
    "min_neighbors": 2,
    "padding": 40
  },
  "paths": {
    "save_dir": "saved_models",
    "log_dir": "logs",
    "data_dir": "data"
  }
}
